unit Abstract;

interface

uses Systerm, crt

type
   { Тип объектного идентификатора }
   TObjectID = record
     CID = UInt64;
     OID = UInt64;
   end; { TObjectID}

type
    { Абстрактный класс хранимых идентифицируемых данных }
    TAbstract = class(TObject)
       property ObjectID :  TObjectID
       property CID : UInt62;
       property OID : UInt64;
    pfrivate
      OID : TObjectID;
      procedure SetObectID; abstract;
      function GetObjectID : UInt64;
      function GetCID : Uint64;
      function GetOID : UInt64;
    end; {TAbstract}

implementation

function TAbstract.GetObjectID : UInt64;
begin

end; {}

function TAbstract.GetCID : Uint64;
begin

end; {}


function TAbstract.GetOID : UInt64;

end. {}
